{
  "name": "cypress-example",
  "version": "1.0.0",
  "description": "Repository showing Cypress tests",
  "main": "index.js",
  "scripts": {
    "test": "docker run -it -e CYPRESS_baseUrl=$CYPRESS_baseUrl -e CYPRESS_updateSnapshots=$CYPRESS_updateSnapshots --ipc=host -v $PWD:/e2e -w /e2e cypress/included:4.11.0",
    "test:ci": "npm run test -- run --record --key $CYPRESS_recordKey"
  },
  "keywords": [
    "cypress",
    "ci"
  ],
  "author": "Leonardo Faria",
  "license": "MIT",
  "devDependencies": {
    "@simonsmith/cypress-image-snapshot": "^9.0.2",
    "cypress": "^13.8.1"
  }
}
